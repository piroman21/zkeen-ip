{
  "routing": {
    "domainStrategy": "IPOnDemand",
      "rules": [
      {
        "inboundTag": ["redirect", "tproxy"],
        "domain": [
          "regexp:^([\\w\\-\\.]+\\.)ua$",
          "ext:zkeen.dat:domains"
        ],
        "outboundTag": "proxy",
        "type": "field"
      },
      {
        "inboundTag": ["redirect", "tproxy"],
        "ip": [
//          "ext:zkeenip.dat:telegram",
          "ext:zkeenip.dat:cloudflare"
        ],
        "outboundTag": "proxy",
        "type": "field"
      },
      {
        "inboundTag": ["redirect", "tproxy"],
        "outboundTag": "direct",
        "type": "field"
      }
    ]
  }
}